{
  "name": "prism.ui",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"./src/**/*.{css,html,js,ts}\"",
    "format:check": "prettier --check \"./src/**/*.{css,html,js,ts}\"",
    "local": "ng serve --configuration local --ssl",
    "publish:local": "npm run build && (robocopy .\\dist c:\\inetpub\\prism.ui /mir /MT) ^& exit 0",
    "publish:dev": "npm run build && powershell ./ci/deploy.ps1 -branch dev",
    "publish:release": "npm run build && powershell ./ci/deploy.ps1 -branch release",
    "publish:master": "npm run build && powershell ./ci/deploy.ps1 -branch master"
  },
  "dependencies": {
    "@angular/animations": "11.2.3",
    "@angular/cdk": "^11.2.4",
    "@angular/common": "11.2.3",
    "@angular/compiler": "11.2.3",
    "@angular/core": "11.2.3",
    "@angular/forms": "11.2.3",
    "@angular/material": "^11.2.4",
    "@angular/platform-browser": "11.2.3",
    "@angular/platform-browser-dynamic": "11.2.3",
    "@angular/router": "11.2.3",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@ngxs/form-plugin": "^3.7.1",
    "@ngxs/router-plugin": "^3.7.1",
    "@ngxs/storage-plugin": "^3.7.1",
    "@ngxs/store": "^3.7.1",
    "bootstrap-sass": "^3.4.1",
    "dayjs": "^1.8.14",
    "file-saver-es": "^2.0.5",
    "lodash-es": "^4.17.21",
    "lodash.template": "^4.5.0",
    "lodash.templatesettings": "^4.2.0",
    "luhn": "^2.3.0",
    "ngx-bootstrap": "4.3.0",
    "ngx-mask": "^11.1.4",
    "rxjs": "^6.6.6",
    "tslib": "^2.0.0",
    "zipcelx": "^1.6.2",
    "chart.js": "2.9.4",
    "ng2-charts": "2.4.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.3",
    "@angular/cli": "^11.2.3",
    "@angular/compiler-cli": "11.2.3",
    "@angular/language-service": "11.2.3",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@types/node": "^12.20.5",
    "codelyzer": "^6.0.0",
    "prettier": "^1.19.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "prettier": {
    "printWidth": 240,
    "singleQuote": true,
    "tabWidth": 4
  }
}
